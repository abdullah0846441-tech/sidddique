        ];

        // Render products
        function renderProducts(list) {
            const grid = document.getElementById('productGrid');
            grid.innerHTML = '';
            list.forEach(p => {
                const col = document.createElement('div'); col.className = 'col-6 col-md-4 col-lg-3';
                col.innerHTML = `
      <div class="card product-card h-100 shadow-sm">
        <div class="position-relative">
          <img loading="lazy" src="${p.img}" alt="${p.title}" class="card-img-top rounded-top">
          ${p.price < 20 ? '<span class="badge badge-sale position-absolute top-0 start-0 m-2 p-2">Sale</span>' : ''}
        </div>
        <div class="card-body d-flex flex-column">
          <h6 class="card-title">${p.title}</h6>
          <p class="card-text text-muted small mb-2">${p.desc}</p>
          <div class="mt-auto d-flex justify-content-between align-items-center">
            <div class="fw-bold">$${p.price.toFixed(2)}</div>
            <div>
              <button class="btn btn-sm btn-outline-secondary me-2" onclick='openQuickView(${p.id})'><i class="bi bi-eye"></i></button>
              <button class="btn btn-sm btn-primary" onclick='addToCart(${p.id},1)'><i class="bi bi-cart-plus"></i></button>
            </div>
          </div>
        </div>
      </div>
    `;
                grid.appendChild(col);
            });
        }

        // Basic client-side cart
        let CART = JSON.parse(localStorage.getItem('pashamob_cart') || '[]');
        function saveCart() { localStorage.setItem('pashamob_cart', JSON.stringify(CART)); updateCartUI(); }
        function addToCart(id, qty = 1) {
            const item = CART.find(i => i.id === id);
            if (item) item.qty += qty; else { const p = PRODUCTS.find(x => x.id === id); CART.push({ id: p.id, title: p.title, price: p.price, qty: qty }); }
            saveCart();
            alert('Added to cart');
        }
        function addToCartFromModal() { const id = Number(document.getElementById('quickViewModal').dataset.pid); const qty = Number(document.getElementById('modalQty').value || 1); addToCart(id, qty); var modal = bootstrap.Modal.getInstance(document.getElementById('quickViewModal')); modal.hide(); }
        function updateCartUI() {
            const count = CART.reduce((s, i) => s + i.qty, 0); document.getElementById('cartCount').textContent = count;
            const items = document.getElementById('cartItems'); items.innerHTML = '';
            let total = 0;
            CART.forEach(it => {
                total += it.price * it.qty;
                const div = document.createElement('div'); div.className = 'd-flex justify-content-between align-items-center mb-2';
                div.innerHTML = `<div><div class="fw-semibold">${it.title}</div><div class="small text-muted">Qty: ${it.qty}</div></div><div class="text-end"><div>$${(it.price * it.qty).toFixed(2)}</div><button class='btn btn-sm btn-link text-danger p-0' onclick='removeFromCart(${it.id})'>Remove</button></div>`;
                items.appendChild(div);
            });
            document.getElementById('cartTotal').textContent = `$${total.toFixed(2)}`;
        }
        function removeFromCart(id) { CART = CART.filter(i => i.id !== id); saveCart(); }
        function toggleCart() { const drawer = document.getElementById('cartDrawer'); if (drawer.style.transform.includes('110%')) drawer.style.transform = 'translateX(0)'; else drawer.style.transform = 'translateX(110%)'; }

        // Quick view modal
        function openQuickView(id) {
            const p = PRODUCTS.find(x => x.id === id); const modal = new bootstrap.Modal(document.getElementById('quickViewModal'));
            document.getElementById('modalImage').src = p.img; document.getElementById('modalTitle').textContent = p.title; document.getElementById('modalDesc').textContent = p.desc; document.getElementById('modalPrice').textContent = `$${p.price.toFixed(2)}`; document.getElementById('modalSku').textContent = 'SKU: ' + p.sku; document.getElementById('quickViewModal').dataset.pid = p.id; modal.show();
        }

        // Search (simple client-side)
        function performSearch() { const q = document.getElementById('searchInput').value.trim().toLowerCase(); if (!q) return renderProducts(PRODUCTS); const filtered = PRODUCTS.filter(p => (p.title + ' ' + p.desc).toLowerCase().includes(q)); renderProducts(filtered); }

        // Sorting
        function sortProducts() { const val = document.getElementById('sortSelect').value; let clone = [...PRODUCTS]; if (val === 'price-asc') clone.sort((a, b) => a.price - b.price); if (val === 'price-desc') clone.sort((a, b) => b.price - a.price); renderProducts(clone); }

        // Subscribe and contact (demo)
        function subscribe() { const email = document.getElementById('emailInput').value || ''; if (!email) return alert('Please enter an email'); alert('Thanks! You are subscribed (demo)'); document.getElementById('emailInput').value = ''; }
        function contactUs() { const name = document.getElementById('contactName').value; const email = document.getElementById('contactEmail').value; const msg = document.getElementById('contactMessage').value; if (!name || !email || !msg) return alert('Please fill all fields'); alert('Thanks, ' + name + ' — we received your message (demo)'); document.getElementById('contactName').value = ''; document.getElementById('contactEmail').value = ''; document.getElementById('contactMessage').value = ''; }

        // Init
        document.getElementById('year').textContent = new Date().getFullYear(); renderProducts(PRODUCTS); updateCartUI();

        // Accessibility: keyboard shortcut to focus search (Alt+S)
        document.addEventListener('keydown', e => { if (e.altKey && e.key.toLowerCase() === 's') { e.preventDefault(); document.getElementById('searchInput').focus(); } });
    </script>

